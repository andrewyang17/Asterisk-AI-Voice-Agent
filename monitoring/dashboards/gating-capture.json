{
  "id": null,
  "title": "Gating & Capture",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "stat",
      "title": "TTS Gating Active (sum)",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "targets": [
        {"expr": "sum(sum_over_time(ai_agent_tts_gating_active[$__range])) or vector(0)", "refId": "A"}
      ]
    },
    {
      "type": "stat",
      "title": "Audio Capture Enabled (sum)",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "targets": [
        {"expr": "sum(sum_over_time(ai_agent_audio_capture_enabled[$__range])) or vector(0)", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Gating Active over time",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "targets": [
        {"expr": "max_over_time(sum without (call_id) (ai_agent_tts_gating_active)[$__rate_interval]) or vector(0)", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Audio Capture Enabled over time",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "targets": [
        {"expr": "max_over_time(sum without (call_id) (ai_agent_audio_capture_enabled)[$__rate_interval]) or vector(0)", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Barge-in Events (rate 5m)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "targets": [
        {"expr": "rate(ai_agent_barge_in_events_total[5m]) or vector(0)", "refId": "A"}
      ]
    }
  ]
}
